  </canvas>
  <hyphae>
    <dyingPixels>100</dyingPixels>
    <euthanasiaPercentage>10</euthanasiaPercentage> <!--%-->
    <dyingOutsidePercentage>20</dyingOutsidePercentage> <!--%-->
    <creationAreaSize>5</creationAreaSize>
    <primalHyphaCount>
      <v>1</v>
      <v>3</v>
      <v>7</v>
      <v>11</v>
      <v>51</v>
      <v>101</v>
      <v>501</v>
      <v>751</v>
      <v>1001</v>
      <v>3001</v>
    </primalHyphaCount>
    <newPrimalHyphaPeriod> <!--seconds-->
      <min>0.0</min>
      <max>0.2</max>
    </newPrimalHyphaPeriod>
    <maxHyphaCount>
      <v>29</v>
      <v>51</v>
      <v>301</v>
      <v>601</v>
      <v>1001</v>
      <v>5001</v>
      <v>10001</v>
      <v>20001</v>
      <v>30001</v>
      <v>40001</v>
      <v>50001</v>
      <v>60001</v>
      <v>70001</v>
      <v>80001</v>
      <v>90001</v>
      <v>100001</v>
      <v>150001</v>
      <v>250001</v>
      <v>250001</v>
      <v>250001</v>
      <v>250001</v>
      <v>400001</v>
