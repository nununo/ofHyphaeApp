<mycelia>
  <framerate>30</framerate>
  <osdColor>
    <r>0</r>
    <g>0</g>
    <b>0</b>
    <a>255</a>
  </osdColor>
  <canvas>
    <width>1280</width>
    <height>720</height>
    <shaderFilename>shaders/alphamask.frag</shaderFilename>
    <backgroundColor>
      <r>255</r>
      <g>255</g>
      <b>255</b>
    </backgroundColor>
  </canvas>
  <mycelium>
    <border>
      <distortion>0.7</distortion>
      <ratioRange>
        <min>1</min>
        <max>2</max>
      </ratioRange>
      <radiusRange>
        <min>400</min>
        <max>800</max>
      </radiusRange>
    </border>
    <hyphae>
      <creationAreaSize>
        <min>10</min>
        <max>10</max>
      </creationAreaSize>
      <primalHyphaCount>
        <min>100</min>
        <max>100</max>
      </primalHyphaCount>
      <newPrimalHyphaPeriod> <!--seconds-->
        <min>0.02</min>
        <max>0.02</max>
      </newPrimalHyphaPeriod>
      <maxHyphaCount>
        <min>20000</min>
        <max>20000</max>
      </maxHyphaCount>
      <hypha>
        <color>
          <r>0</r>
          <g>0</g>
          <b>0</b>
          <a>10</a>
        </color>
        <speed> <!--pixels/s-->
          <min>4</min>
          <max>4</max>
        </speed>
        <speedVariation> <!--%-->
          <min>20</min>
          <max>20</max>
        </speedVariation>
        <maxForkAngle>
          <min>10</min>
          <max>10</max>
        </maxForkAngle>
        <maxBendAngle>
          <min>3</min>
          <max>3</max>
        </maxBendAngle>
        <radiusTolerance> <!--%-->
          <min>10</min>
          <max>10</max>
        </radiusTolerance>
      </hypha>
    </hyphae>
  </mycelium>
</mycelia>
