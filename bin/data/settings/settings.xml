<rhizopus>
  <canvas>
    <framerate>30</framerate>
    <osdColor>
      <r>0</r>
      <g>0</g>
      <b>0</b>
      <a>255</a>
    </osdColor>
    <width>1280</width>
    <height>720</height>
    <shaderFilename>shaders/alphamask.frag</shaderFilename>
    <backgroundColor>
      <r>255</r>
      <g>255</g>
      <b>255</b>
    </backgroundColor>
  </canvas>
  <hyphae>
    <creationAreaSize>
      <min>10</min>
      <max>10</max>
    </creationAreaSize>
    <primalHyphaCount>
      <v>1</v>
      <v>5</v>
      <v>10</v>
      <v>50</v>
      <v>100</v>
      <v>500</v>
    </primalHyphaCount>
    <newPrimalHyphaPeriod> <!--seconds-->
      <min>0.0</min>
      <max>0.2</max>
    </newPrimalHyphaPeriod>
    <maxHyphaCount>
      <v>1</v>
      <v>2</v>
      <v>10</v>
      <v>100</v>
      <v>1000</v>
    </maxHyphaCount>
    <border>
      <distortion>
        <min>0.1</min>
        <max>1</max>
      </distortion>
      <ratioVariation>
        <min>1</min>
        <max>2</max>
      </ratioVariation>
      <radius>
        <min>100</min>
        <max>500</max>
      </radius>
    </border>
    <hypha>
      <color>
        <r>0</r>
        <g>0</g>
        <b>0</b>
        <a>20</a>
      </color>
      <speed> <!--pixels/s-->
        <min>4</min>
        <max>4</max>
      </speed>
      <speedVariation> <!--%-->
        <min>20</min>
        <max>20</max>
      </speedVariation>
      <maxForkAngle>
        <min>10</min>
        <max>10</max>
      </maxForkAngle>
      <maxBendAngle>
        <min>3</min>
        <max>30</max>
      </maxBendAngle>
      <radiusTolerance> <!--%-->
        <min>10</min>
        <max>10</max>
      </radiusTolerance>
    </hypha>
  </hyphae>
</rhizopus>
