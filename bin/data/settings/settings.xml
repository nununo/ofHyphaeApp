    <border>
      <distortion>0.2</distortion>
      <ratioVariation>
        <min>1</min>
        <max>4</max>
      </ratioVariation>
      <radius>
        <min>200</min>
        <max>600</max>
      </radius>
    </border>
    <hypha>
      <color>
        <r>255</r>
        <g>255</g>
        <b>255</b>
        <a>10</a>
      </color>
      <speed>4</speed> <!--pixels/s-->
      <speedVariation>10</speedVariation> <!--%-->
      <pixelOverlap>0.75</pixelOverlap>
      <maxForkAngle>
        <min>10</min>
        <max>10</max>
      </maxForkAngle>
