<mycelia>
  <framerate>10</framerate>
  <osdColor>
    <r>0</r>
    <g>0</g>
    <b>0</b>
    <a>255</a>
  </osdColor>
  <canvas>
    <width>1280</width>
    <height>720</height>
    <shaderFilename>shaders/alphamask.frag</shaderFilename>
    <backgroundColor>
      <r>255</r>
      <g>255</g>
      <b>255</b>
    </backgroundColor>
  </canvas>
  <mycelium>
    <radiusRange>
      <min>300</min>
      <max>1000</max>
    </radiusRange>
    <conidia>
      <active>0</active>
      <conidium>
        <lifespan>2000</lifespan>
        <delay>10</delay>
        <recenterRatio>0.98</recenterRatio>
      </conidium>
    </conidia>
    <hyphae>
      <active>1</active>
      <creationAreaSize>3</creationAreaSize>
      <primalHyphaCount>100</primalHyphaCount>
      <newPrimalHyphaPeriod>0.01</newPrimalHyphaPeriod> <!--seconds-->
      <maxHyphaCount>20000</maxHyphaCount>
      <hypha>
        <color>
          <r>0</r>
          <g>0</g>
          <b>0</b>
          <a>2</a>
        </color>
        <speed>0.4</speed>
        <speedVariation>20</speedVariation> <!--%-->
        <distortion>1.0</distortion>
        <maxForkAngle>10</maxForkAngle>
        <maxBendAngle>3</maxBendAngle>
        <radiusTolerance>10</radiusTolerance> <!--%-->
      </hypha>
    </hyphae>
  </mycelium>
</mycelia>
