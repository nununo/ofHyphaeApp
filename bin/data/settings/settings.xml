<mycelia>
  <framerate>60</framerate>
  <osdColor>
    <r>0</r>
    <g>0</g>
    <b>0</b>
    <a>255</a>
  </osdColor>
  <canvas>
    <width>1280</width>
    <height>720</height>
    <shaderFilename>shaders/alphamask.frag</shaderFilename>
    <maskFilename>images/bread_profile_mask.png</maskFilename>
    <maskColor>
      <r>255</r>
      <g>255</g>
      <b>255</b>
    </maskColor>
    <backgroundColor>
      <r>255</r>
      <g>255</g>
      <b>255</b>
    </backgroundColor>
  </canvas>
  <mycelium>
    <lifespan>99999</lifespan>
    <radiusRange>
      <min>20</min>
      <max>100</max>
    </radiusRange>
    <conidia>
      <active>0</active>
      <conidium>
        <lifespan>2000</lifespan>
        <delay>10</delay>
        <recenterRatio>0.98</recenterRatio>
      </conidium>
    </conidia>
    <hyphae>
      <active>1</active>
      <creationAreaSize>0</creationAreaSize>
      <primalHyphaCount>10</primalHyphaCount>
      <newHyphaPeriod>20</newHyphaPeriod>
      <maxHyphaCount>10000</maxHyphaCount>
      <hypha>
        <color>
          <r>0</r>
          <g>0</g>
          <b>0</b>
          <a>1</a>
        </color>
        <maxLifespan>200</maxLifespan>
        <speed>0.1</speed>
        <speedVariation>10</speedVariation> <!--%-->
        <maxHeight>100</maxHeight>
        <distortion>1.0</distortion>
        <forkAgeRatio>2</forkAgeRatio>
        <maxForkAngle>90</maxForkAngle>
        <maxBendAngle>1</maxBendAngle>
        <fertilityRateRatio>1</fertilityRateRatio>
        <fertilityRatePower>2</fertilityRatePower>
        <radiusTolerance>50</radiusTolerance> <!--%-->
      </hypha>
    </hyphae>
  </mycelium>
</mycelia>