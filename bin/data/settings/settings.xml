<rhizopus>
  <canvas>
    <framerate>15</framerate>
    <antialiasing>1</antialiasing>
    <mourningTime>3</mourningTime>
    <fadeoutTime>3</fadeoutTime>
    <idleTime>2</idleTime>
    <logToFile>1</logToFile>
    <osdColor>
      <r>255</r>
      <g>255</g>
      <b>255</b>
      <a>255</a>
    </osdColor>
    <backgroundColor>
      <r>0</r>
      <g>0</g>
      <b>0</b>
