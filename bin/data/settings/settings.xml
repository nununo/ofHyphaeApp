<rhizopus>
  <canvas>
    <framerate>30</framerate>
    <osdColor>
      <r>0</r>
      <g>0</g>
      <b>0</b>
      <a>255</a>
    </osdColor>
<!--
    <size id="4K">
      <width>3840</width>
      <height>2160</height>
    </size>
-->
    <size id="MacBook Pro 15 Retina">
      <width>3840</width>
      <height>2160</height>
    </size>
    <backgroundColor>
      <r>255</r>
      <g>255</g>
      <b>255</b>
    </backgroundColor>
    <mourningTime>3</mourningTime>
    <fadeoutTime>5</fadeoutTime>
  </canvas>
  <hyphae>
    <dyingTime>5</dyingTime>
    <creationAreaSize>
      <min>5</min>
      <max>5</max>
    </creationAreaSize>
    <primalHyphaCount>
      <v>1</v>
      <v>5</v>
      <v>10</v>
      <v>50</v>
      <v>100</v>
      <v>500</v>
    </primalHyphaCount>
    <newPrimalHyphaPeriod> <!--seconds-->
      <min>0.0</min>
      <max>0.2</max>
    </newPrimalHyphaPeriod>
    <maxHyphaCount>
      <v>1</v>
      <v>2</v>
      <v>5</v>
      <v>10</v>
      <v>20</v>
      <v>50</v>
      <v>100</v>
      <v>200</v>
      <v>500</v>
      <v>1000</v>
      <v>2000</v>
      <v>5000</v>
      <v>10000</v>
      <v>15000</v>
      <v>20000</v>
      <v>30000</v>
      <v>40000</v>
      <v>50000</v>
      <v>60000</v>
      <v>70000</v>
      <v>80000</v>
      <v>90000</v>
      <v>100000</v>
    </maxHyphaCount>
    <border>
      <distortion>
        <min>0.1</min>
        <max>1</max>
      </distortion>
      <ratioVariation>
        <min>1</min>
        <max>2</max>
      </ratioVariation>
      <radius>
        <min>200</min>
        <max>500</max>
      </radius>
    </border>
    <hypha>
      <color>
        <r>0</r>
        <g>0</g>
        <b>0</b>
        <a>20</a>
      </color>
      <speed> <!--pixels/s-->
        <min>20</min>
        <max>20</max>
      </speed>
      <speedVariation> <!--%-->
        <min>10</min>
        <max>10</max>
      </speedVariation>
      <maxForkAngle>
        <min>10</min>
        <max>10</max>
      </maxForkAngle>
      <maxBendAngle>
        <min>3</min>
        <max>10</max>
      </maxBendAngle>
      <maxBentAngle>
        <min>80</min>
        <max>80</max>
      </maxBentAngle>
      <radiusTolerance> <!--%-->
        <min>30</min>
        <max>50</max>
      </radiusTolerance>
      <pixelOverlap>
        <min>0.9</min>
        <max>0.9</max>
      </pixelOverlap>
    </hypha>
  </hyphae>
</rhizopus>
