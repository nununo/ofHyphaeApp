<rhizopus>
  <canvas>
    <framerate>15</framerate>
    <osdColor>
      <r>255</r>
      <g>255</g>
      <b>255</b>
      <a>255</a>
    </osdColor>
    <backgroundColor>
      <r>0</r>
      <g>0</g>
      <b>0</b>
    </backgroundColor>
    <mourningTime>3</mourningTime>
    <fadeoutTime>5</fadeoutTime>
    <idleTime>2</idleTime>
  </canvas>
  <hyphae>
    <dyingPixels>150</dyingPixels>
    <creationAreaSize>
      <min>5</min>
      <max>5</max>
    </creationAreaSize>
    <primalHyphaCount>
      <v>1</v>
      <v>3</v>
      <v>5</v>
      <v>7</v>
      <v>11</v>
      <v>51</v>
      <v>101</v>
      <v>501</v>
      <v>1001</v>
      <v>3001</v>
    </primalHyphaCount>
    <newPrimalHyphaPeriod> <!--seconds-->
      <min>0.0</min>
      <max>0.2</max>
    </newPrimalHyphaPeriod>
    <maxHyphaCount>
      <v>1</v>
      <v>3</v>
      <v>5</v>
      <v>7</v>
      <v>11</v>
      <v>13</v>
      <v>17</v>
      <v>19</v>
      <v>23</v>
      <v>29</v>
      <v>51</v>
      <v>101</v>
      <v>201</v>
      <v>301</v>
      <v>501</v>
      <v>601</v>
      <v>1001</v>
      <v>2001</v>
      <v>3001</v>
      <v>5001</v>
      <v>10001</v>
      <v>15001</v>
      <v>20001</v>
      <v>30001</v>
      <v>40001</v>
      <v>50001</v>
      <v>60001</v>
      <v>70001</v>
      <v>80001</v>
      <v>90001</v>
      <v>100001</v>
    </maxHyphaCount>
    <border>
      <distortion>
        <min>0.2</min>
        <max>0.2</max>
      </distortion>
      <ratioVariation>
        <min>0.1</min>
        <max>4</max>
      </ratioVariation>
      <radius>
        <min>200</min>
        <max>400</max>
      </radius>
    </border>
    <hypha>
      <color>
        <r>255</r>
        <g>255</g>
        <b>255</b>
        <a>10</a>
      </color>
      <speed> <!--pixels/s-->
        <min>3</min>
        <max>3</max>
      </speed>
      <speedVariation> <!--%-->
        <min>10</min>
        <max>10</max>
      </speedVariation>
      <maxForkAngle>
        <min>10</min>
        <max>10</max>
      </maxForkAngle>
      <maxBendAngle>
        <min>3</min>
        <max>10</max>
      </maxBendAngle>
      <maxBentAngle>
        <min>80</min>
        <max>80</max>
      </maxBentAngle>
      <pixelOverlap>
        <min>0.99</min>
        <max>0.99</max>
      </pixelOverlap>
      <fertilityRatio>
        <min>1</min>
        <max>1</max>
      </fertilityRatio>
    </hypha>
  </hyphae>
</rhizopus>
